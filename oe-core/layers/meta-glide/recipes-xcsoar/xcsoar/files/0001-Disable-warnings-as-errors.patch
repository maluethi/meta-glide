Subject: [PATCH] Disable warnings as errors

---
 build/warnings.mk | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/build/warnings.mk b/build/warnings.mk
index c4aa673..8ac5314 100644
--- a/build/warnings.mk
+++ b/build/warnings.mk
@@ -25,8 +25,8 @@ WERROR ?= $(DEBUG)
 endif
 
 ifeq ($(WERROR),y)
-CXXFLAGS += -Werror
-CFLAGS += -Werror
+#CXXFLAGS += -Werror
+#CFLAGS += -Werror
 endif
 
 #CXXFLAGS += -pedantic
